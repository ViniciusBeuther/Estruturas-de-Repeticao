#ALGORITMO183
#Entrar com 10 numeros e imprimir o quadrado dos mesmos

prog algoritmo183
       int c, n1;
       
           para(c <- 0; c < 10; c++)
           {
                  imprima "\nDigite um numero: ";
                  leia n1;
                  imprima "\nO quadrado: ", n1 * n1, "\n\n";
           }

fimprog